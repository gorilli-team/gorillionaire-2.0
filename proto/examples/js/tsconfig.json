{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "../..",
    "moduleResolution": "node"
  },
  "include": [
    "*.ts",
    "../../gen/v1/typescript/proto/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
} 